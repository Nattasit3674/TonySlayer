<!-- update.ejs -->
<!DOCTYPE html>
<html>
  <head>
    <title>Update name</title>
    <link rel="stylesheet" href="../css/style.css" />
  </head>
  <body>
    <h1>Update name</h1>
    <form method="post" action="/update2/<%= aus.id_new %>">
      <label for="name">Name:</label>
      <input type="text" id_new="name" name="name" value="<%= aus.name %>" />
      <br />
      <label for="last">Last:</label>
      <input type="text" id_new="last" name="last" value="<%= aus.last %>" />
      <br />
      <input type="submit" value="Update2" />
    </form>
    <div class="link-center">
      <a href="/aus/<%= aus.id_new %>">View</a>
      <a href="/delete2/<%= aus.id_new %>">Delete</a>
      <a href="/auss">Back to List</a>
    </div>
  </body>
</html>